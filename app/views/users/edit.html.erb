<h1>User inpo</h1>
<%= form_with model: @user do |f| %>
  <label for="inputName">Name</label><br>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前" %><br>
  <label for="inputImage">Image</label><br>
  <%= f.file_field :user_icon, placeholder: "プロフィール画像", accept: "image/*" %><br>
  <label for="inputIntroduction">Introduction</label><br>
  <%= f.text_area :user_introduction, autofocus: true, id:"inputIntroduction" %> <br>
  <%= f.submit "Update User" %>
<% end %>