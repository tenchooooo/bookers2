<h1>Editing Book</h1>
<%= form_with model: @book do |f| %>
  <label for="inputTitle">Title</label><br>
  <%=f.text_field :title, autofocus:true, id:"inputTitle" %> <br>
  <label for="inputBody">Opinion</label><br>
  <%= f.text_area :body, autofocus:true, id:"inputBody" %> <br>
  <%= f.submit "Update Book" %>
<% end %>